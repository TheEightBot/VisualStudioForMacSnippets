<?xml version="1.0" encoding="utf-8"?>
<CodeTemplates version="3.0">
  <CodeTemplate version="2.0">
    <Header>
      <_Group>C#</_Group>
      <Version />
      <MimeType>text/x-csharp</MimeType>
      <Shortcut>propfull</Shortcut>
      <_Description>full property getter setter</_Description>
      <TemplateType>Unknown</TemplateType>
    </Header>
    <Variables>
      <Variable name="property">
        <Default>someProp</Default>
      </Variable>
      <Variable name="type">
        <Default>string</Default>
      </Variable>
      <Variable name="MyProperty">
        <Default>SomeProp</Default>
      </Variable>
    </Variables>
    <Code><![CDATA[private $type$ _$property$;

    public $type$ $MyProperty$
    {
        get => $property$;
        set => $property$ = value;
    }]]></Code>
  </CodeTemplate>
</CodeTemplates>